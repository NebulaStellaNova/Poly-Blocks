                            <data>
                                <item text="Player"></item>
                                <item text="Opponent"></item>
                            </data>
                        </dropdown>
                        <label text="Strumline" style="margin-top: 5px;" />
                        <dropdown width="85">
                            <data>
                                <item text="Up"></item>
                                <item text="Down"></item>
                            </data>
                        </dropdown>
                        <number-stepper id="strumpx" pos="0" step="1" verticalAlign="center"
                            tooltip="ADD ONE LATER????" />
                        <label text="px" style="margin-top: 5px;" />
                    </hbox>
                </button>

                <button styleName="block">
                    <vbox>
                        <hbox>
                            <label text="Tween" style="margin-top: 5px;" />
                            <dropdown width="85">
                                <data>
                                    <item text="Player"></item>
                                    <item text="Opponent"></item>
                                </data>
                            </dropdown>
                            <label text="Strumline" style="margin-top: 5px;" />
                            <dropdown width="85">
                                <data>
                                    <item text="Up"></item>
                                    <item text="Down"></item>
                                </data>
                            </dropdown>
                            <label text="px" style="margin-top: 5px;" />
                        </hbox>
                        <hbox>
                            <label text="Ease Type:" style="margin-top: 5px;" />
                            <dropdown width="85">
                                <data>
                                    <item text="Sine"></item>
                                    <item text="Quad"></item>
                                </data> 
                            </dropdown>
                            <label text="Direction" style="margin-top: 5px;" />
                            <dropdown width="85">
                                <data>
                                    <item text="In"></item>
                                    <item text="Out"></item>
                                    <item text="In Out"></item>
                                </data>
                            </dropdown>
                            <label text="px" style="margin-top: 5px;" />
